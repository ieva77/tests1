        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bylist"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="653.947">
    <Grid Margin="0,0,-101,-48">
        <TextBox x:Name="BuyListItemName" HorizontalAlignment="Left" Height="87" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="147" RenderTransformOrigin="1.107,3.618" Margin="88,36,0,0" TextChanged="BuyListItemName_TextChanged">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleY="-1" ScaleX="-1"/>
                    <SkewTransform/>
                    <RotateTransform Angle="-180.234"/>
