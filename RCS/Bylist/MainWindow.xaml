            </TextBox.RenderTransform>
        </TextBox>
        <Button x:Name="AddListItemButton" Click="AddListItemButton_Click" Content="Pievienot pirkumu" HorizontalAlignment="Left" Margin="282,41,0,0" VerticalAlignment="Top" Width="192" RenderTransformOrigin="0.187,-1.136" Height="83"/>
        <TextBlock x:Name="BuyItemName" HorizontalAlignment="Left" Margin="94,139,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" RenderTransformOrigin="-0.338,0.197" Height="49" Width="140">
